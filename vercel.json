{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/styles.css",
      "dest": "api.py"
    },
    {
      "src": "/script.js",
      "dest": "api.py"
    },
    {
      "src": "/index.html",
      "dest": "api.py"
    },
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "env": {
    "PRODUCTION": "true"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ],
  "github": {
    "enabled": false,
    "autoJobCancelation": true
  },
  "cleanUrls": true,
  "trailingSlash": false
}